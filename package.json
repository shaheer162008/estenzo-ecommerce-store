{
  "name": "estenzo-ecommerce-store",
  "version": "1.0.0",
  "description": "Premium UAE Fashion Ecommerce Store built with Next.js 15, TypeScript, and MongoDB",
  "keywords": [
    "ecommerce",
    "nextjs",
    "typescript",
    "mongodb",
    "uae",
    "fashion",
    "shopping",
    "react",
    "tailwindcss"
  ],
  "author": {
    "name": "Estenzo Team",
    "email": "dev@estenzo.com",
    "url": "https://estenzo.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/yourusername/estenzo-ecommerce-store#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/estenzo-ecommerce-store.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/estenzo-ecommerce-store/issues"
  },
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next dist out",
    "test": "echo \"No tests specified yet\" && exit 0",
    "prepare": "husky install || true",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u",
    "build:analyze": "ANALYZE=true npm run build",
    "db:seed": "node scripts/seed.js",
    "postinstall": "npm run type-check"
  },
